                        <p class="text-sm ms-1">website</p>
                    </div>
                    <p class="text-muted"><%=user.website ? user.website : "no website"%></p>
                </div>
                <div class="border-bottom d-flex justify-content-between p-2 link">
                    <% if(user.instagram) { %>
                    <a href="https://<%=user.instagram%>"></a>
                    <% } %>
